{
    "name": "dbtedman/congress",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "fruitcake/laravel-cors": "2.0.5",
        "guzzlehttp/guzzle": "7.4.1",
        "laravel/framework": "8.82.0",
        "laravel/sanctum": "2.14.0",
        "laravel/tinker": "2.7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "facade/ignition": "2.17.4",
        "fakerphp/faker": "1.19.0",
        "laravel/sail": "1.13.1",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "5.11.0",
        "phpunit/phpunit": "9.5.13",
        "squizlabs/php_codesniffer": "3.6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "@php phpcs --standard=PSR12 ./app ./config ./routes"
        ],
        "format": [
            "@php phpcbf --standard=PSR12 ./app ./config ./routes"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
